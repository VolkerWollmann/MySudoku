<Window x:Class="MySudoku.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="WO Sudoku" Height="800" Width="850" MinHeight="500" MinWidth="550" >
    <Grid x:Name="MySudokuGame" MinWidth="550" MinHeight="450">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="9*" MinWidth="50"  />
            <ColumnDefinition Width="120" MinWidth="150" MaxWidth="150" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*" MinHeight="50" />
        </Grid.RowDefinitions>
    </Grid>
</Window>
